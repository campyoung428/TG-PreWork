const bobsFollowers = ['Rick', 'Anthony', 'Issa', 'Ryder'];
const tinasFollowers = ['Terah', 'Anthony', 'Issa'];
const mutualFollowers = []
for(let i =0; i< bobsFollowers.length; i++) {
  for(let j = 0; j< tinasFollowers.length; j++) { 
    if(bobsFollowers[i] === tinasFollowers[j]) {
      console.log('Both loops have the name: Anthony, Issa' + [j] );
}
    
  }
};
